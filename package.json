{
  "name": "mapi.wkt",
  "version": "1.0.0",
  "description": "An easy to use wrapper for Google Maps API",
  "main": "mapi.wkt.js",
  "scripts": {
    "build-normal": "webpack src/mapi.wkt.js mapi.wkt.js",
    "build-min": "webpack -p src/mapi.wkt.js mapi.wkt.min.js",
    "build": "npm run build-normal && npm run build-min",
    "dev": "webpack -w src/mapi.wkt.js mapi.wkt.js"
  },
  "repository": {
    "type": "git",
    "url": "github.com/mapijs/mapi.wkt.git"
  },
  "keywords": [
    "google",
    "maps"
  ],
  "author": "Thiago Ribeiro - thiagofribeiro@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.2.1",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-preset-es2015": "^6.1.18",
    "eslint": "^1.10.3",
    "eslint-loader": "^1.1.1",
    "mapi": "github:mapijs/mapi",
    "webpack": "^1.12.9"
  },
  "dependencies": {
    "jquery": "^2.1.4",
    "underscore": "^1.8.3",
    "wicket": "github:arthur-e/Wicket"
  }
}
